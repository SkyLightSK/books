{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:create": "npm run typeorm migration:create -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "fixtures": "fixtures ./fixtures --config src/ormconfig.ts --sync --require=ts-node/register --require=tsconfig-paths/register"
  },
  "dependencies": {
    "@nestjs/common": "6.11.1",
    "@nestjs/core": "6.11.1",
    "@nestjs/platform-express": "6.11.1",
    "@nestjs/typeorm": "6.2.0",
    "pg": "^7.17.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rxjs": "6.5.4",
    "typeorm": "0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "6.14.1",
    "@nestjs/schematics": "6.8.2",
    "@nestjs/testing": "6.11.1",
    "@types/express": "4.17.2",
    "@types/node": "7.10.9",
    "@types/supertest": "2.0.8",
    "@types/ws": "7.2.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "25.0.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.20.1",
    "typeorm-fixtures-cli": "^1.4.2",
    "typescript": "3.7.2"
  }
}
